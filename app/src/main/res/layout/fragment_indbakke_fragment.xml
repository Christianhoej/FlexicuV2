<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.Indbakke_fragment"
    android:background="@color/hvid"
    android:id="@+id/indbakke_fragment_frame">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.design.widget.BottomNavigationView
            android:id="@+id/indbakke_nav"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:layout_alignParentBottom="true"
            android:background="@color/hvid"
            app:itemIconTint="@color/nav_item_colors_green"
            app:itemTextColor="@color/nav_item_colors_green"
            app:menu="@menu/indbakke_items">
        </android.support.design.widget.BottomNavigationView>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/flex_green"
            >

        </View>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/til_aftaler_frame">
        </FrameLayout>
    </LinearLayout>



