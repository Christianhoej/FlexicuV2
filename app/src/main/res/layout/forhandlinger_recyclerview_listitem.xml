<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/forhandlinger_recyclerview_listitem">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Udlejning eller indlejning"
            android:textSize="18dp"
            android:id="@+id/forhandlinger_type"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/forhandlinger_navn"
            android:textSize="18dp"
            android:text="Medarbjederens navn"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/forhandlinger_periode"
            android:textSize="18dp"
            android:text="02/01/19-18/01/19"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/forhandlinger_virk"
            android:textSize="18dp"
            android:text="Hans Hansen APS"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/forhandlinger_pris"
            android:textSize="18dp"
            android:text="Samlet pris"/>




    </LinearLayout>

</RelativeLayout>